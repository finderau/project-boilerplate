{
  "name": "new-project",
  "version": "1.0.0",
  "description": "New Project",
  "private": true,
  "main": "js/index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --progress",
    "watch": "NODE_ENV=development webpack -d --progress --watch",
    "build:dev": "NODE_ENV=development webpack -d --progress",
    "clean": "rm -rf ./build && mkdir ./build",
    "start": "NODE_ENV=development webpack-dev-server -d --open",
    "pretest": "npm run lint:js && npm run lint:sass",
    "test": "npm run test:unit",
    "test:unit": "rm -rf ./coverage && NODE_ENV=test ./node_modules/.bin/nyc ./node_modules/.bin/mocha --require babel-register --require ignore-styles ./testConfig.js \"js/**/*.spec.js\" && npm run report",
    "test:unit-watch": "NODE_ENV=test ./node_modules/.bin/mocha --require babel-register --require ignore-styles ./testConfig.js \"js/**/*.spec.js\" --watch",
    "report": "nyc report --reporter=text-lcov > coverage/coverage.lcov",
    "report:codecov": "echo \"Error: Codecov id is missing\" && exit 1",
    "lint:sass": "sass-lint 'styles/**.scss' --verbose --no-exit",
    "lint:js": "eslint ./js",
    "lint:watch": "npm-run-all --parallel lint:js lint:css"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "babel-core": "~6.9.0",
    "babel-eslint": "~7.1.1",
    "babel-loader": "~6.2.10",
    "babel-plugin-istanbul": "~3.1.2",
    "babel-plugin-ramda": "~1.1.6",
    "babel-plugin-transform-class-properties": "~6.11.5",
    "babel-plugin-transform-react-jsx": "~6.8.0",
    "babel-plugin-transform-react-jsx-source": "~6.9.0",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.16.0",
    "babel-preset-stage-3": "~6.17.0",
    "babel-register": "~6.18.0",
    "codecov": "~1.0.1",
    "css-loader": "~0.26.1",
    "eslint": "~3.13.0",
    "eslint-config-airbnb-base": "~11.0.1",
    "eslint-import-resolver-webpack": "~0.6.0",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-react": "~6.0.0",
    "eslint-plugin-standard": "~1.3.2",
    "extract-text-webpack-plugin": "~2.0.0-rc.1",
    "file-loader": "~0.10.0",
    "ignore-styles": "~5.0.1",
    "image-webpack-loader": "~3.2.0",
    "node-sass": "~4.4.0",
    "npm-run-all": "~4.0.1",
    "nyc": "~10.1.2",
    "postcss-loader": "~1.2.2",
    "react-addons-test-utils": "~15.4.0",
    "sass-lint": "~1.10.2",
    "sass-loader": "~4.1.1",
    "style-loader": "~0.13.1",
    "webpack": "~2.2.0",
    "webpack-dev-server": "~1.16.2"
  },
  "dependencies": {
    "chai": "~3.5.0",
    "redux-immutable-state-invariant": "~1.2.4",
    "enzyme": "~2.4.0",
    "mocha": "~3.0.0",
    "fetch-mock": "~5.9.3",
    "luna": "git+ssh://git@github.com:finderau/luna.git#master",
    "luna-react": "git+ssh://git@github.com:finderau/luna-react.git",
    "native-promise-only": "~0.8.1",
    "ramda": "~0.23.0",
    "react": "~15.4.1",
    "react-dom": "~15.4.1",
    "react-redux": "~4.4.5",
    "redux": "~3.6.0",
    "redux-mock-store": "~1.2.1",
    "redux-thunk": "~2.1.0",
    "sinon": "~1.17.5",
    "whatwg-fetch": "~1.0.0"
  }
}
